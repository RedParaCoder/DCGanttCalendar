    <data-set name="Subjects" display-name-key="Dataset for Subjects" cds-data-set-options="displayCommandBar:true;displayViewSelector:true;displayQuickFind:true">
      <property-set name="ID"
        display-name-key="ID"
        description-key="ID of Subjects"
        of-type="Whole.None"
        usage="bound" />
        <property-set name="Title"
        display-name-key="Title"
        description-key="ID of Subjects"
        of-type="Whole.None"
