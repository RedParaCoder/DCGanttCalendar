    <property name="endDate" display-name-key="end date" description-key="end date for element" of-type="DateAndTime.DateOnly" usage="bound" required="true" />
    <property name="dayWidth" display-name-key="day width" description-key="individual day width" of-type="Decimal" usage="bound" required="true" />
