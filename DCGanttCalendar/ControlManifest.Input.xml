    <!-- <property-set name="subjectId" display-name-key="Subject Id" description-key="Id of the records of Subjects" of-type="Whole.None" usage="input" required="true" />
    <property-set name="subjectTitle" display-name-key="Subject Title" description-key="Title of the records of Subjects" of-type="SingleLine.Text" usage="input" required="true" /> -->
